<!DOCTYPE html>
<html>
    <head>

        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}{% endblock %}</title>
        <link href="{{ asset('/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
        <link href="{{ asset('/bootstrap/css/bootstrap-responsive.min.css') }}" rel="stylesheet" media="screen">
    </head>
    <body>

        {% block body %}{% endblock %}

{% block javascripts %}
    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script src="{{ asset('/js/main.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/js/jquery.timer.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/js/jstorage.min.js') }}" type="text/javascript"></script>
{% endblock %}

    </body>
